<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
	<!-- general display strings, no prefixes -->
	<string name="space">" "</string>
	<string name="separator">、</string>
	<string name="final_separator">、</string>
	<string name="root_folder">""</string>
	<string name="back_button">帰る</string>

	<!-- notification display strings, prefixed by "notif" -->
	<string name="notif_channel_name">アラーム</string>
	<string name="notif_channel_description">アラームはここに全部送る。</string>
	<string name="notif_ticker">きたきたー このアラーム</string>
	<string name="notif_description">ここにクリックすると、スヌーズも解除もできる</string>
	<string name="notif_dismiss">解除</string>
	<string name="notif_snooze">スヌーズ</string>
	<string name="notif_service_description">アラームを開始する、通知音を再生する。</string>

	<!-- RecyclerViewHolder context menu display strings, prefixed by "listable_menu"
		 If you change the order of these, make sure to change it in RecyclerViewAdapter
	-->
	<string name="listable_menu_delete_alarm">アラーム削除</string>
	<string name="listable_menu_delete_folder">ディレクトリー削除</string>
	<string-array name="listable_menu_alarms">
		<item>@string/listable_menu_delete_alarm</item>
	</string-array>
	<string-array name="listable_menu_folders">
		<item>@string/listable_menu_delete_folder</item>
	</string-array>

	<!-- MainActivity display strings, prefixed by "main" -->
	<string name="main_no_alarms_text">何もないよ</string>
	<string name="main_next_alarm">次は
		<xliff:g id="main_date_string" example="3/14/22">%2$s</xliff:g>
		<xliff:g id="time" example="12:01">%3$s</xliff:g>に
		「<xliff:g id="name" example="名前">%1$s</xliff:g>」</string>
	<string name="main_no_next_alarm">次アラームがない</string>
	<string name="main_date_string"><xliff:g id="date" example="3/14/22">%s</xliff:g></string>
	<string name="main_date_string_today">本日</string>
	<string name="main_date_string_tomorrow">明日</string>

	<!-- MainActivity description strings, prefixed by "main," suffixed by "description" -->
	<string name="main_alarm_description">アラーム追加</string>
	<string name="main_folder_description">ディレクトリー追加</string>
	<string name="main_settings_description">設定</string>

	<string name="main_edit_folder_description">ディレクトリー編集</string>

	<!-- PrefsActivity strings, prefixed by "prefs" -->
	<string name="prefs_title">設定</string>
	<string name="prefs_theme">テーマ: </string>
	<string name="prefs_theme_description">Theme is separate from light/dark mode. Both light and
		dark mode have versions of these color palettes.</string>
	<string name="prefs_system_dark">Use system dark mode</string>
	<string name="prefs_dark_override">Use dark mode (if not following system settings)</string>
	<string name="prefs_menu_placement">Put back/save buttons above content (otherwise below)</string>

	<!-- themes, prefixed by "theme" -->
	<string name="theme_beach">Sandy beach</string>
	<string name="theme_candy">Candy</string>
	<string name="theme_mint">Mint</string>
	<!-- TODO: use grey instead of gray in *not* EN-US -->
	<string name="theme_grey">Grayscale</string>

	<!-- these have to match the indices in the array themes_list -->
	<integer name="theme_beach">0</integer>
	<integer name="theme_candy">1</integer>
	<integer name="theme_mint">2</integer>
	<integer name="theme_grey">3</integer>

	<string-array name="themes_list">
		<item>@string/theme_beach</item>
		<item>@string/theme_candy</item>
		<item>@string/theme_mint</item>
		<item>@string/theme_grey</item>
	</string-array>
</resources>
